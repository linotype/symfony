{
    "name": "linotype/symfony",
    "type": "symfony-bundle",
    "description": "a component monolith",
    "keywords": [
        "linotype",
        "symfony"
    ],
    "homepage": "https://linotype.dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannick Armspach",
            "email": "yannick.armspach@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Linotype\\Bundle\\LinotypeBundle\\": "",
            "Linotype\\Helper\\": "../../../linotype/Helper/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "oomphinc/composer-installers-extender": "^2.0",
        "doctrine/common": "^2.13 || ^3.0",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^1.12|^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.6,>=2.6.3",
        "doctrine/persistence": "^1.3.3 || ^2.0",
        "nikic/php-parser": "^4.3",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/yaml": "5.2.*",
        "symfony/asset": "^4.4|^5.0",
        "symfony/cache": "^4.4|^5.0",
        "symfony/config": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/deprecation-contracts": "^2.0",
        "symfony/event-dispatcher": "^4.4|^5.0",
        "symfony/filesystem": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/http-foundation": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/intl": "^4.4|^5.0",
        "symfony/polyfill-php73": "^1.13",
        "symfony/property-access": "^4.4|^5.0",
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/string": "^5.0",
        "symfony/translation": "^4.4|^5.0",
        "symfony/twig-bundle": "^4.4|^5.0",
        "symfony/uid": "^5.1",
        "linotype/core": "^0|^1"
    },
    "require-dev": {
        "symfony/framework-bundle": "^4.4.17|^5.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/twig-bundle": "^4.4.17|^5.0",
        "symfony/var-dumper": "^4.4.17|^5.0",
        "symfony/maker-bundle": "^1.29"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.1-dev"
        }
    },
    "minimum-stability": "dev"
}
