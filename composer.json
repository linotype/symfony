{
    "name": "linotype/symfony",
    "type": "symfony-bundle",
    "description": "a component monolith",
    "keywords": [
        "linotype",
        "symfony"
    ],
    "homepage": "https://linotype.dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannick Armspach",
            "email": "yannick.armspach@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Linotype\\Bundle\\LinotypeBundle\\": "",
            "Linotype\\Helper\\": "../../../linotype/Helper/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "symfony/console": "^5.2.*",
        "symfony/dotenv": "^5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.2.*",
        "symfony/orm-pack": "^v2.1.0",
        "symfony/twig-bundle": "^5.2.*",
        "symfony/yaml": "^5.2.*",
        "symfony/config": "^4.4.17|^5.0",
        "symfony/dependency-injection": "^4.4.17|^5.0",
        "symfony/http-kernel": "^4.4.17|^5.0",
        "symfony/asset": "^5.2.*",
        "linotype/core": "^0|^1"
    },
    "require-dev": {
        "symfony/framework-bundle": "^4.4.17|^5.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/twig-bundle": "^4.4.17|^5.0",
        "symfony/var-dumper": "^4.4.17|^5.0",
        "symfony/maker-bundle": "^1.29"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.1-dev"
        }
    },
    "minimum-stability": "dev"
}
