    <div class="panel-footer">
        <ul class="menu menu-nav">
            <li class="menu-item">
                <a href="/options">
                    <div class="custom-icon">{% include '@Linotype/Icons/settings.svg' %}</div> <span class="menu-item-title">Options</span>
